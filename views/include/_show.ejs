<%for(var i=0; i<users.length;i++){ %>
  <tr>
    <td><%=i+1%></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].email%></td>
    <td><%=users[i].gender%></td>
    <td><%=users[i].status%></td>
    <td>
      <a
        href="/updateUser/?id=<%=users[i]._id%>"
        class="btn btn border-shadow update"
      >
        <span class="text-gradinet"><i class="fas fa-pencil-alt"></i></span>
      </a>
      <a class="btn btn border-shadow delete"  data-id =<%=users[i]._id%> >
        <span class="text-gradinet"><i class="fas fa-times"></i></span>
      </a>
    </td>
  </tr>

  <% } %>
